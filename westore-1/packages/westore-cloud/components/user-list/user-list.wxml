<view class="title">用户列表</view>
<view class="list" wx:for="{{user}}" wx:for-item="item">
    <view class="list-item" >
        <text class="user-name">{{index + 1}}. {{item.name}}</text>
        <text class="log-item">{{item.city}}</text>
        <text class="log-item">{{item.age}}</text>
        <text>{{user.item.gender===1?'男':'女'}}</text>
        <button ontap="onTap" data-index="{{index}}">删除</button>
    </view>  
</view>